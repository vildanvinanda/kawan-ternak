<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingActivityMenu"
    android:background="@color/white"
    >

    <RelativeLayout
        android:id="@+id/titleEdukasi"
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:background="#FAB832"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:layout_centerVertical="true"
            >
            <ImageView
                android:id="@+id/backbtnsetting"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/ic_row_left"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Setting"
                android:textSize="16dp"
                android:textColor="#fff"
                android:fontFamily="@font/poppinsmedium"
                android:layout_centerHorizontal="true"/>

        </RelativeLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titleEdukasi"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="20dp"
        android:orientation="vertical"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsmedium"
            android:text="Profil Akun"
            android:textColor="#242424"
            android:textSize="16dp" />

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/icpass"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/lockkey"
                android:layout_centerInParent="true"
                />
            <RelativeLayout
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/icpass"
                android:layout_marginStart="10dp"
                >
                <TextView
                    android:id="@+id/updatepass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Ubah Password"
                    android:textColor="#242424"
                    android:textSize="16dp"
                    />
                <TextView
                    android:layout_below="@+id/updatepass"
                    android:id="@+id/ketpass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Perbarui password demi keamanan anda"
                    android:textColor="#979797"
                    android:textSize="12dp"
                    />
            </RelativeLayout>

            <ImageView
                android:id="@+id/rowright"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:background="@drawable/ic_expand_right"
                android:layout_alignParentEnd="true"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/icprofil"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/accountdua"
                android:layout_centerInParent="true"
                />
            <RelativeLayout
                android:layout_marginTop="10dp"
                android:id="@+id/title2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/icprofil"
                android:layout_marginStart="10dp"
                >
                <TextView
                    android:id="@+id/updateprofil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Ubah Profil"
                    android:textColor="#242424"
                    android:textSize="16dp"
                    />
                <TextView
                    android:layout_below="@+id/updateprofil"
                    android:id="@+id/ketprofil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Perbarui/Lengkapi identitas anda "
                    android:textColor="#979797"
                    android:textSize="12dp"
                    />
            </RelativeLayout>

            <ImageView
                android:id="@+id/rowright2"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:background="@drawable/ic_expand_right"
                android:layout_alignParentEnd="true"
                />

        </RelativeLayout>
        <TextView
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsmedium"
            android:text="Pusat Bantuan"
            android:textColor="#242424"
            android:textSize="16dp" />
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/ictlp"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/iconohp"
                android:layout_centerInParent="true"
                />
            <RelativeLayout
                android:id="@+id/title3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/ictlp"
                android:layout_marginStart="10dp"
                >
                <TextView
                    android:id="@+id/bantuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Hubungi Kami"
                    android:textColor="#242424"
                    android:textSize="16dp"
                    />
                <TextView
                    android:layout_below="@+id/bantuan"
                    android:id="@+id/ketbantuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Hubungi kami disini"
                    android:textColor="#979797"
                    android:textSize="12dp"
                    />
            </RelativeLayout>

            <ImageView
                android:id="@+id/rowright3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:background="@drawable/ic_expand_right"
                android:layout_alignParentEnd="true"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/icfaq"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/ic_circle_question"
                android:layout_centerInParent="true"
                />
            <RelativeLayout
                android:id="@+id/title4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/icfaq"
                android:layout_marginStart="10dp"
                >
                <TextView
                    android:id="@+id/faq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="FAQ"
                    android:textColor="#242424"
                    android:textSize="16dp"
                    />
                <TextView
                    android:layout_below="@+id/faq"
                    android:id="@+id/ketfaq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Pertanyaan yang sering ditanyakan"
                    android:textColor="#979797"
                    android:textSize="12dp"
                    />
            </RelativeLayout>

            <ImageView
                android:id="@+id/rowright4"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:background="@drawable/ic_expand_right"
                android:layout_alignParentEnd="true"
                />

        </RelativeLayout>
        <TextView
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsmedium"
            android:text="Aplikasi"
            android:textColor="#242424"
            android:textSize="16dp" />
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/icVERSI"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/ic_circle_information"
                android:layout_centerInParent="true"
                />
            <RelativeLayout
                android:id="@+id/title5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/icVERSI"
                android:layout_marginStart="10dp"
                >
                <TextView
                    android:id="@+id/vapp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Vaersi Aplikasi"
                    android:textColor="#242424"
                    android:textSize="16dp"
                    />
                <TextView
                    android:layout_below="@+id/vapp"
                    android:id="@+id/ketvapp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="0.1 Beta"
                    android:textColor="#979797"
                    android:textSize="12dp"
                    />
            </RelativeLayout>


        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <ImageView
                android:id="@+id/icout"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentStart="true"
                android:background="@drawable/ic_arrow_right_from_bracket"
                android:layout_centerInParent="true"
                />
            <RelativeLayout
                android:id="@+id/title6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/icout"
                android:layout_marginStart="10dp"
                >
                <TextView
                    android:id="@+id/out"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppinsregular"
                    android:text="Keluar"
                    android:textColor="#242424"
                    android:textSize="16dp"
                    />
            </RelativeLayout>

            <ImageView
                android:id="@+id/rowright6"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:background="@drawable/ic_expand_right"
                android:layout_alignParentEnd="true"
                />

        </RelativeLayout>
    </LinearLayout>

    <TextView
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinsmedium"
        android:text="Kawan Ternak"
        android:textColor="#3A3A3A"
        android:textSize="12dp"
        />
</RelativeLayout>