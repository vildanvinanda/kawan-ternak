                        >
                        <TextView
                            android:id="@+id/txtBelidari"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Beli Dari"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="25"
                            android:id="@+id/inputBD"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane10"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputBD"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplatePeristiwa"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tamplateBD"

                        >
                        <TextView
                            android:id="@+id/txtperistiwa"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Keterangan"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:id="@+id/inputPristiwa"
                            android:layout_below="@id/txtperistiwa"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane11"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputPristiwa"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                </RelativeLayout>


                <RelativeLayout
