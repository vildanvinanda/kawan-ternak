                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtTTL"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Tanggal Lahir"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="10"
                            android:id="@+id/inputTTL"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane6"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputTTL"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateTB"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tamplateTTL"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtTB"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Tanggal Beli"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="10"
                            android:id="@+id/inputTB"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane7"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputTB"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateUmur"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tamplateTB"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtEUmur"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Estimasi Umur"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
