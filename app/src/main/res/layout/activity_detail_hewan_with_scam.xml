<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailHewanWithScam"
    android:background="@color/white"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <RelativeLayout
                android:id="@+id/titleKeuangan"
                android:layout_width="match_parent"
                android:layout_height="82dp"
                >
                <RelativeLayout
                    android:id="@+id/containertitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="20dp"
                    android:layout_centerVertical="true"
                    >

                    <ImageView
                        android:id="@+id/backbtnkeuangan"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:background="@drawable/ic_row_left"
                        android:backgroundTint="@color/black"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Detail Hewan"
                        android:textSize="16dp"
                        android:textColor="#000"
                        android:fontFamily="@font/poppinsmedium"
                        android:layout_centerHorizontal="true"/>
                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_below="@+id/titleKeuangan"
                android:id="@+id/form"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="20dp"
                android:paddingBottom="100dp"
                >

                <androidx.cardview.widget.CardView
                    android:id="@+id/gambar"
                    android:layout_width="match_parent"
                    android:layout_height="208dp"
                    android:layout_centerHorizontal="true"
                    app:cardCornerRadius="20dp"
                    >
                    <ImageView
                        android:id="@+id/imgdetailhewan"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ic_gambarempty"/>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/idDH"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:layout_below="@+id/gambar"
                    android:visibility="gone"
                    />

                <RelativeLayout
                    android:id="@+id/tamplateNoKandang"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/gambar"
                    android:layout_marginTop="27dp">

                    <TextView
                        android:id="@+id/jdlnokandang"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerInParent="true"
                        android:fontFamily="@font/poppinsregular"
                        android:text="No Kandang"
                        android:textColor="#000"
                        android:textSize="14dp" />

                    <EditText
                        android:id="@+id/inputnokandang"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:fontFamily="@font/poppinsmedium"
                        android:inputType="number"
                        android:maxLength="5"
                        android:singleLine="true"
                        android:text="02"
                        android:textColor="#000"
                        android:textSize="14dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_marginTop="10dp"
                    android:layout_below="@id/tamplateNoKandang"
                    android:id="@+id/kontener1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#F6F6F6"
                    >
                    <TextView
                        android:id="@+id/pertanyaan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Indentitas Hewan"
                        android:textSize="14dp"
                        android:textColor="#242424"
                        android:fontFamily="@font/poppinssemibold"
                        />

                    <ImageView
                        android:id="@+id/rowrightDH"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerInParent="true"
                        android:background="@drawable/ic_expand_right"
                        android:rotation="90" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/countainerIdentitasH"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/kontener1"
                    >

                    <RelativeLayout
                        android:id="@+id/tamplateNamaH"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtnama"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Hewan"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="30"
                            android:id="@+id/inputnamahewan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputnamahewan"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateNP"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tamplateNamaH"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtnamap"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Pemilik"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="30"
                            android:id="@+id/inputnamapemilik"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane0"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputnamapemilik"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateJK"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tamplateNP"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtJK"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Jenis Kelamin"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:singleLine="true"
                            android:maxLength="10"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/inputjk"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane2"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputjk"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateStatus"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tamplateJK"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtstatus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Status"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="10"
                            android:id="@+id/inputStatus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane3"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputStatus"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
