                            android:singleLine="true"
                            android:maxLength="15"
                            android:id="@+id/inputEUmur"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane8"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputEUmur"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateHB"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tamplateUmur"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/txtHargaBeli"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Harga Beli"
                            android:fontFamily="@font/poppinsregular"
                            android:textSize="14dp"
                            android:textColor="#000"
                            android:layout_centerInParent="true"
                            android:layout_alignParentStart="true"
                            />
                        <EditText
                            android:layout_alignParentEnd="true"
                            android:singleLine="true"
                            android:maxLength="15"
                            android:id="@+id/inputHB"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:fontFamily="@font/poppinsmedium"
                            android:textSize="14dp"
                            android:textColor="#000"
                            />
                        <View
                            android:id="@+id/lane9"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/inputHB"
                            android:layout_width="match_parent"
                            android:visibility="visible"
                            android:layout_height="1dp"
                            android:background="#DDD"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/tamplateBD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
